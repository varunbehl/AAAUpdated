package com.aaa.mobile.lib;
import java.util.Hashtable;

import com.aaa.mobile.page.SecondaryProblemDiscriptionPage;
import org.testng.ITestContext;
import com.aaa.accelerators.ActionEngineMobile;

public class SecondaryProblemDiscriptionLib extends ActionEngineMobile {
	public ITestContext testcontext;
	public void setTestContext(ITestContext testContext) {
		this.testcontext = testContext;
	}

	/**
	 * method name :selectProblemType
	 *discription : select Problem Type
	 *param testdata
	 * throws Throwable
	 */	
	public void selectProblemType() throws Throwable  {
		waitForVisibilityOfElement(SecondaryProblemDiscriptionPage.batteryProblem, "select problem type ");
		click(SecondaryProblemDiscriptionPage
				.batteryProblem, "select problem type");
	}

	/**
	 * method name :needJumpStart
	 *discription : needJumpStart
	 * throws Throwable
	 */
	public void needJumpStart() throws Throwable  {
		waitForVisibilityOfElement(SecondaryProblemDiscriptionPage.needJumpStartLabel, "choose jump start");
		click(SecondaryProblemDiscriptionPage.needJumpStartLabel, "need jump start");
	}




}
